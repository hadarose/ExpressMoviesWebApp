<!DOCTYPE html>
<html>
  <body>
    <h1><%= title %></h1>

    <ul>
      <% users.forEach(function(user) { %>

      <li>
        <h3><%= user.username %></h3>
        <a href="/users/add/<%= user.username %>">Update</a><br />
        <a href="/users/delete/<%= user.username %>">Delete</a>
      </li>
      <br />

      <% }) %>
    </ul>

    <br />
    <a href="/users/add">Add New User</a>

    <br /><br />
    <a href="/menu">Back To Menu</a>
  </body>
</html>
